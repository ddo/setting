// Place your key bindings in this file to overwrite the defaults
[
    
    { "key": "ctrl+g",  "command": "editor.action.nextMatchFindAction", "when": "editorFocus" },
    { "key": "ctrl+shift+g", "command": "editor.action.previousMatchFindAction", "when": "editorFocus" },
    { "key": "ctrl+,", "command": "workbench.action.openGlobalSettings"},
    { "key": "ctrl+i", "command": "go.import.add", "when": "editorFocus && editorLangId==go" },
    { "key": "ctrl+alt+d", "command": "editor.action.previewDeclaration" },
    { "key": "ctrl+shift+b", "command": "HookyQR.beautify", "when": "editorFocus" }
]
